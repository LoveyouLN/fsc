package com.org.fsc.ssh.teble.dao;

import java.util.List;
import java.util.Map;

public interface TableDao {

	//查询所有桌子信息
	List<?> selAllTable();

	//后台查询所有桌子信息
	List<?> selAllTableInfo(Map<String,String> map);

	//新增一个桌子
	int addOneTable(Map<String,String> map);

	//修改一个桌子
	int updOneTable(Map<String,String> map);

	//查询一个桌子信息
	List<?> selOneTableInfo(Map<String,String> map);

	//逻辑删除一个桌子
	int delOneTable(Map<String,String> map);

	//修改一个桌子的状态
	int updOneTableState(Map<String,String> map);

}
